{
  "ConnectionStrings": {
    "DefaultConnection": "mongodb://localhost:27017/dojo"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Features": {
    "FingerPrintValidation": true
  },
  "OpenidConfiguration": {
    "issuer": "http://localhost:5257",
    "authorization_endpoint": "http://localhost:5257/auth",
    "token_endpoint": "http://localhost:5257/api/v1/auth/token",
    "token_endpoint_auth_methods_supported": [
      "client_secret_basic",
      "private_key_jwt"
    ],
    "token_endpoint_auth_signing_alg_values_supported": [
      "RS256",
      "ES256"
    ],
    "acr_values_supported": [
      "urn:mace:incommon:iap:silver",
      "urn:mace:incommon:iap:bronze"
    ],
    "response_types_supported": [
      "code",
      "code id_token",
      "id_token",
      "token id_token"
    ],
    "subject_types_supported": [
      "public",
      "pairwise"
    ],
    "userinfo_encryption_enc_values_supported": [
      "A128CBC-HS256",
      "A128GCM"
    ],
    "id_token_signing_alg_values_supported": [
      "RS256",
      "ES256",
      "HS256"
    ],
    "id_token_encryption_alg_values_supported": [
      "RSA1_5",
      "A128KW"
    ],
    "id_token_encryption_enc_values_supported": [
      "A128CBC-HS256",
      "A128GCM"
    ],
    "request_object_signing_alg_values_supported": [
      "none",
      "RS256",
      "ES256"
    ],
    "display_values_supported": [
      "page",
      "popup"
    ],
    "claim_types_supported": [
      "normal",
      "distributed"
    ],
    "scopes_supported": [
      "openid",
      "profile",
      "email",
      "address",
      "phone",
      "offline_access"
    ],
    "claims_supported": [
      "sub",
      "iss",
      "auth_time",
      "acr",
      "name",
      "given_name",
      "family_name",
      "nickname",
      "profile",
      "picture",
      "website",
      "email",
      "email_verified",
      "locale",
      "zoneinfo"
    ],
    "claims_parameter_supported": true,
    "service_documentation": "http://localhost:5257/connect/service_documentation.html",
    "ui_locales_supported": [
      "en-US",
      "en-GB",
      "en-CA",
      "fr-FR",
      "fr-CA"
    ]
  },
  "AllowedHosts": "*"
}
